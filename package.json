{
  "name": "sixtynine-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "genkey": "node genkey.mjs",
    "build:server": "tsc && NODE_ENV=production vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:client": "tsc && NODE_ENV=production vite build --outDir dist/client",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "schema": "get-graphql-schema https://appwisp.com/api/graphql > schema.graphql",
    "relay": "relay-compiler",
    "start": "node server.js",
    "serve": "NODE_ENV=production bun server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@froxz/vite-plugin-s3": "^1.5.2",
    "@nextui-org/react": "^2.6.5",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.13.4",
    "get-graphql-schema": "^2.1.2",
    "highlight.js": "^11.9.0",
    "http-proxy-middleware": "^3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^17.0.0",
    "react-relay-network-modern": "^6.2.2",
    "react-relay-network-modern-ssr": "^1.4.0",
    "react-router-dom": "^6.23.1",
    "rehype-highlight": "^7.0.0",
    "relay-runtime": "^17.0.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwindcss": "^3.4.4",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@mdx-js/rollup": "^3.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^16.0.6",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-plugin-relay": "^17.0.0",
    "eslint": "^9.5.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "relay-compiler": "^17.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-mdx": "^3.6.0",
    "vite-plugin-relay": "^2.1.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.18.0"
  }
}
